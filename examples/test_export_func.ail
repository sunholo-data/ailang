-- Test module with export func declarations
module examples/test_export_func

-- This function is exported
export func add(x: int, y: int) -> int {
  x + y
}

-- This function is NOT exported (no export keyword)
func _helper(x: int) -> int {
  x * 2
}

-- Another exported function
export func double(x: int) -> int {
  _helper(x)
}

-- Test the functions locally
print("add(3, 4) = " ++ show(add(3, 4)))
print("double(5) = " ++ show(double(5)))