-- test_instances.ail
-- Demonstrates type class instance resolution

-- Simple arithmetic that requires Num[Int]
let x = 2 + 3 in

-- Comparison that requires Ord[Int] 
let y = if 5 > 3 then "greater" else "less" in

-- Equality that uses Eq derived from Ord
let z = 4 == 4 in

-- String concatenation (no class needed)
print("x = " ++ show(x))
print("y = " ++ y)
print("z = " ++ show(z))