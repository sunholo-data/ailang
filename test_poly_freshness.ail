-- Test polymorphic type variable freshness
-- Each use of a polymorphic function should get fresh type variables

-- Define polymorphic identity inline
let id = \x. x in

-- Use at Int
show_Int(id(42)) ++

-- Use at String  
show_String(id("hello")) ++

-- Nested uses - both should be fresh
show_Int(id(id(7))) ++

-- In combination with other functions
show_String("prefix" ++ id("-suffix"))